    "def disemvowel(sr):\n",
    "    vowels = \"AaEeIiOoUu\"\n",
    "    for vow in vowels:\n",
    "        while vow in sr:\n",
    "            sr = sr[:sr.index(vow)] + sr[sr.index(vow)+1:]\n",
    "    return sr\n",
    "\n",
    "examp = 'This website is for losers LOL!'\n",
    "examp = disemvowel(examp)\n",
    "print(examp)"
